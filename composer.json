{
    "name": "epalshin/object-to-graphql",
    "description": "Utility helper for generating GraphQL types with webonyx/graphql-php",
    "keywords": [
        "palshin",
        "object_to_graphql"
    ],
    "homepage": "https://github.com/palshin/php-object-to-graphql",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Palshin",
            "email": "eo.palshin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "webonyx/graphql-php": "^14.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Palshin\\ObjectToGraphQL\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Palshin\\ObjectToGraphQL\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
